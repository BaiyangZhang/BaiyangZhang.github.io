Cell[TextData[{
 "Define  the Hamiltonian operator in terms of x and p first, then convert it \
to a and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["a", "\[Dagger]"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4abc52d3-6aab-c44c-a534-cb8831a89dba"],
 "."
}], "Text",
 CellChangeTimes->{{3.9317566936472073`*^9, 
  3.9317567217709255`*^9}},ExpressionUUID->"cb969ea5-41a9-d343-a267-\
1af51bdfd018"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Ham", "=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"p", "=", 
       RowBox[{"\[ImaginaryI]", 
        SqrtBox[
         FractionBox[
          RowBox[{"\[HBar]", " ", "m", " ", "\[Omega]"}], "2"]], 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", "a", ")"}], "\[Dagger]"], "-", "a"}], ")"}]}]}], ",", 
      RowBox[{"x", "=", 
       RowBox[{
        SqrtBox[
         FractionBox["\[HBar]", 
          RowBox[{"2", " ", "m", " ", "\[Omega]"}]]], 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", "a", ")"}], "\[Dagger]"], "+", "a"}], ")"}]}]}]}], 
     "}"}], ",", 
    RowBox[{
     RowBox[{"CommutatorExpand", "[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"p", "^", "2"}], 
         RowBox[{"2", "m"}]], "+", 
        RowBox[{
         FractionBox["1", "2"], "m", " ", 
         RowBox[{"\[Omega]", "^", "2"}], " ", 
         RowBox[{"x", "^", "2"}]}]}], ",", 
       RowBox[{"ReverseOrdering", "->", "True"}]}], "]"}], "//", 
     "Simplify"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.9317566829740257`*^9, 3.931756688057169*^9}, {
  3.931757142983061*^9, 3.9317572982566776`*^9}, {3.9317573468774624`*^9, 
